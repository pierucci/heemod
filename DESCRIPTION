Package: heemod
Title: Models for Health Economic Evaluation
Version: 0.9.1.9004
Authors@R: c(
    person("Antoine", "Filipovic-Pierucci", email = "pierucci@gmail.com", role = c("aut", "cre")),
    person("Kevin", "Zarca", role = "aut"),
    person("Matthew", "Wiener", role = "ctb"),
